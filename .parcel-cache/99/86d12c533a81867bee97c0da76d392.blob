.site-nav{
    display: block;
    width: 100%;
    margin: 0 auto;
    background: rgba(20, 12, 12, 0.3);
    color: rgb(131,131,131);
    padding: 8px 0px;
    transition: all 0.5s;

    

}
.sticky{
    background: rgba(20, 12, 12, 0.5);
    color: black;
    z-index: 1;
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.25);
    padding: 4px 0px;


}
.site-nav::after{
    content: "";
    height: 5px;
    opacity: 1;
    pointer-events: none;
    position: absolute;
    right: 0;
    width: 100%;
    z-index: 2050;
    bottom: 0;
    left: 0;
}
