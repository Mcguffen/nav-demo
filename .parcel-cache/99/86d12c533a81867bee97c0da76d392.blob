.clearfix{
    zoom: 1;
}
.clearfix::after{
    content: '';
    display: block;
    clear: both;
}
nav>div>ul>li{
    float: left;
    margin-left: 17px;
    margin-right: 17px;
    position: relative;
}
nav>div>ul>li>a{
    font-size: 12px;
    font-weight: bold;
    color: inherit;
    border-bottom: 2px solid transparent;
    display: block;
    padding-bottom: 5px;
    position: relative;
}
nav>div>ul>li.active>a::after{
    position: absolute;
    content: '';
    display: block;
    top: 100%;
    left: 0;
    width: 100%;
    background: #e06567;
    height: 3px;
    animation: menuSlide 0.3s;
}
@keyframes menuSlide {
    0%{width: 0;}
    100%{width: 100%;}
}
.site-nav .subMenu{
    display: none;
    position: absolute;
    right: 0;
    top: 100%;
    background: rgba(200,200,200,0.9);
    color: #3d4451;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);

}
.site-nav .subMenu > li{
    white-space: nowrap;
    padding: 5px 10px;
    cursor: pointer;
}
.site-nav li.active >.subMenu{
    display: block;
    animation: subMenuSlide 0.3s;

}
@keyframes subMenuSlide {
    0%{margin-right: 100%;}
    100%{margin-right: 0%;}
}
