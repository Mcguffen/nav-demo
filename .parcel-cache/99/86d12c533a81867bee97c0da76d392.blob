*{margin: 0;padding: 0;box-sizing: border-box;}
a{text-decoration: none;}
kbd{border: 1px solid rgb(131,131,131);height: 40px;width: 40px;display: inline-block;text-transform: uppercase;
position: relative;

}
.key{
    background:  rgb(241,241,241);
    color: rgb(131,131,131);
    border-radius: 4px;
    box-shadow: 0 0 0 1px rgb(175,175,175),0 0 0 1px rgb(168,168,168),0 3px 0 2px rgb(175,175,175);
    display: inline-flex;
    align-items: center;
    justify-content: center;
}
.key > img{
    width: 12px;
    height: 12px;
}
.row{
    margin: 8px;
}
.row .key{
    margin: 0 4px;
}
kbd > button{
    position: absolute;
    right: 0;
    bottom: 0;
    display: none;
}
kbd > img{
    position: absolute;
    left: 0;
    bottom: 0;
    display: none;
}
kbd:hover > img{
    display: inline-block;
}
kbd:hover > button{
    display: inline-block;
}
#keyboard{
    display: inline-block;
    background: rgba(200,200,200,0.9);
    border-radius: 4px;
}
body{
