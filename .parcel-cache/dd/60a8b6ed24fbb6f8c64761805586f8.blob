<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>我的导航</title>
    <link rel="stylesheet" href="563fd50d718577864c67592ae4509f0a">
</head>
<body>
    <header></header>
        <main>
            <div class="wrapper" id="keyboard"></div>
        </main>
        

        
    <footer></footer>
    <script data-parcel-key="5f787741a0c740166f0e0fcc4bfe5143">
        var key = [
            [
                'esc','F1','F2','F3','F4','F5',
                'F6','F7','F8','F9','F10','F11',
                'F12','Print','Pause','Delete',
            
            ],
            
            [
                '`','1','2','3','4','5','6',
                '7','8','9','0','-','=','Back','Home',
            ],
            
            [
                'Tab','Q','W','E','R','T','Y',
                'U','I','O','P','[',']','.','\\','PageUp'
            ],
            
            [
                'Ctrl','A','S','D','F','G','H',
                'J','K','L',';','\'','Enter','PageDown',
            ],
            
            [
                'Shift','Z','X','C','V','B','N',
                'M',',','.','/','Shift','↑','End',
            ],
            
            [
                'Caps','option','command','NIZ','Alt','Fn',
                'Ctrl','←','↓','→',
            ],
            
        ]

        var hash = {
            
                'esc':'网址','F1':'网址','F2':'网址','F3':'网址','F4':'网址','F5':'网址',
                'F6':'网址','F7':'网址','F8':'网址','F9':'网址','F10':'网址','F11':'网址',
                'F12':'网址','Print Screen Sys Rq':'网址','Pause Break':'网址','Delete':'网址',
            
                '`':'网址','1':'网址','2':'网址','3':'网址','4':'网址','5':'网址','6':'网址',
                '7':'网址','8':'网址','9':'网址','0':'网址','-':'网址','=':'网址','Backspace':'网址','Home':'网址',
            
                'Tab':'网址','q':'qq.com','w':'网址','e':'网址','r':'网址','t':'网址','y':'网址',
                'u':'网址','i':'网址','o':'网址','p':'网址','[':'网址',']':'网址','.':'网址','\\':'网址','PageUp':'网址',
            
                'Ctrl':'网址','a':'网址','s':'网址','d':'网址','f':'网址','g':'网址','h':'网址',
                'j':'网址','k':'网址','l':'网址',';':'网址','\'':'网址','Enter':'网址','PageDown':'网址',
            
                'Shift':'网址','z':'网址','X':'网址','C':'网址','V':'网址','B':'网址','N':'网址',
                'M':'网址',',':'网址','.':'网址','/':'网址','Shift':'网址','↑':'网址','End':'网址',
            
                'Caps':'网址','option':'网址','command':'网址','long':'网址','Alt':'网址','Fn':'网址',
                'Ctrl':'网址','←':'网址','↓':'网址','→':'网址',
            
        }

        keyboard = document.getElementById("keyboard")

        // 遍历keys，生成kbd标签。
        index = 0
        while(index < key.length){
            // index 0 1 2 3 4 5
            // row 代表第 1 2 3 4 5个数组
            row = key[index]
            // console.log(row)
            div = document.createElement("div")
            keyboard.appendChild(div)

            index2 = 0
            while(index2 < row.length){
                kbd = document.createElement('kbd')
                kbd.textContent = row[index2]
                div.appendChild(kbd)

                index2 += 1

            }
            
             index += 1

        }
        
        document.onkeypress = function(fdsfsfsf){
            // 监听键盘按下的按键
                key = fdsfsfsf['key'] // qwertyuiop[]\
                website = hash[key]
                console.log(website)
        }
    </script>
</body>
</html>