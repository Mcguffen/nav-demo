.col-auto>.navbar{
    float: left;
}
.col-auto>.aside-toggle{
    float: left;
    cursor: pointer;
}
.aside-toggle .lines>div{
    position: absolute;
    background: #fff;
    width: 22px;
    height: 2.5px;
    border-radius: 100px;
}
.aside-toggle .lines .line-1{
